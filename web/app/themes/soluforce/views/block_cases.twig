{% block cases %}
  <div class="container">
    <h1>{{section.acf_block_title}}</h1>
    {{section.acf_block_content}}
    {% if cases %}
      <div class="cases row">
        {% for case in cases %}
          <article class="case col-xs-12 col-sm-4">
            {% if case.acf_case_image %}
              <div class="entry-image">
                <img src="{{TimberImage(case.acf_case_image).src}}" alt="{{TimberImage(case.acf_case_image).alt}}" />
              </div>
            {% endif %}  
            <div class="overlay">
              <div class="entry-content">
                <h5>{{case.post_title}}</h5>
              </div>
            </div>
          </article>  
        {% endfor %} 
      </div>  
    {% endif %}
  </div>  
{% endblock %}