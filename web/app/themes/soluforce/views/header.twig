<header class="banner navbar navbar-default navbar-fixed-top" role="banner">
  <nav id="top-nav" class="collapse navbar-collapse">
    <div class="container">
      <div class="row">
        <div class="search col-sm-8">
          <ul class="nav navbar-right navbar-nav top-nav">
            <li class=""><a class="search" href="#" title=""><i class="fa fa-search"></i></a></li>
          </ul>  
        </div>
        <div class="service col-sm-4">
          <ul class="nav navbar-nav top-nav">
            <li class=""><a href="">Contact</a></li>
          </ul>
      </div>  
    </div>  
  </nav>
  <div class="container">
    <div class="navbar-header row">
      <div class="logo col-sm-4">
        <a class="brand" href="{{site.url}}">
          <img src="{{theme.uri}}/dist/svg/soluforce_logo.svg" onerror="this.onerror=null; this.src='{{theme.uri}}/dist/images/soluforce_logo.png'" alt="{{site.title}}">
        </a>
      </div>
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">{{ __('Toggle navigation', 'gloam') }}</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      {% if main_menu.get_items %}
        <nav id="main-nav" class="col-sm-8 collapse navbar-collapse">
          <ul class="nav navbar-nav">
          {% for item in main_menu.get_items %}
            <li class="{{item.classes | join(' ')}}"><a href="{{item.get_link | relative}}" title="{{item.title}}" target="{{item.target}}">{{item.title}}</a></li>
          {% endfor %}
          </ul> 
        </nav>
      {% endif %}
    </div>
    {% if main_menu.get_items %}
      {% for item in main_menu.get_items %}
        {% if item.current or item.current_item_parent and item.get_children %}
          <nav id="sub-nav" class="collapse navbar-collapse">
            <ul class="nav navbar-nav sub-nav">
              {% for child in item.get_children %}
                <li class="{{child.classes | join(' ')}}"><a href="{{child.get_link | relative}}">{{child.title}}</a></li>
              {% endfor %}
            </ul>
          </nav>
        {% endif %} 
      {% endfor %}
    {% endif %}    
  </div>
</header>